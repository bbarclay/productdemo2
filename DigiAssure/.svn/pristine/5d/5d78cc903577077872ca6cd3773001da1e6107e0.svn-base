package com.cg.digi.model;

public class Device {
	
	private String deviceName;
	private String deviceID;
	private String displayStatus;
	private String model;
	private String deviceOs;
	private String osVersion;
	
	
	public String getDisplayStatus() {
		return displayStatus;
	}
	public void setDisplayStatus(String displayStatus) {
		this.displayStatus = displayStatus;
	}
	public Device(){
		
	}
	
	public Device(String deviceName, String deviceID, String displayStatus,String deviceOs, String osVersion) {
		this.deviceName = deviceName;
		this.deviceID = deviceID;
		this.displayStatus = displayStatus;
		this.deviceOs = deviceOs;
		this.osVersion = osVersion;
		
	}
	
	public String getDeviceName() {
		return deviceName;
	}
	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}
	public String getDeviceID() {
		return deviceID;
	}
	public void setDeviceID(String deviceID) {
		this.deviceID = deviceID;
	}
	
	

	public String getDeviceOs() {
		return deviceOs;
	}
	public void setDeviceOs(String deviceOs) {
		this.deviceOs = deviceOs;
	}
	public String getOsVersion() {
		return osVersion;
	}
	public void setOsVersion(String osVersion) {
		this.osVersion = osVersion;
	}
	@Override
	public String toString() {
		return "Device [deviceName=" + deviceName + ", deviceID=" + deviceID
				+ ", displayStatus=" + displayStatus + ", deviceOs=" + deviceOs + ", osVersion=" + osVersion + "]";
	}
	
}
